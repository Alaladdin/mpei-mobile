{
  "expo": {
    "name": "MPEI Mobile",
    "slug": "mpei-mobile",
    "version": "0.0.1",
    "description": "",
    "githubUrl": "https://github.com/Alaladdin/mpei-mobile",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "primaryColor": "#1f1b3a",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1f1b3a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.agrabah.mpeiMobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1f1b3a"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "themeColor": "#1f1b3a"
    }
  }
}
